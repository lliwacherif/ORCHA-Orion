# AURA Backend Configuration
# Copy this file to .env and update with your actual values

# LM Studio Configuration
LMSTUDIO_URL=http://192.168.1.37:1234

# CRITICAL: Set this to the EXACT model name shown in LM Studio
# For Gemma 3, it might be something like:
# - "gemma-2-9b-it" (for Gemma 2)
# - "gemma-3-vision" (if it's a vision variant)
# - Or whatever name shows in LM Studio's Developer tab
LMSTUDIO_VISION_MODEL=YOUR_MODEL_NAME_HERE

# Database
DATABASE_URL=postgresql+asyncpg://postgres:1234@localhost:5432/orcha_db

# Redis
REDIS_URL=redis://localhost:6379/0

# JWT Settings
JWT_SECRET_KEY=your-secret-key-change-in-production-min-32-chars
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Service URLs
OCR_SERVICE_URL=http://localhost:8001
RAG_SERVICE_URL=http://localhost:8002

# Timeouts
LM_TIMEOUT=500
RAG_TIMEOUT=15
OCR_TIMEOUT=60
